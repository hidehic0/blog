<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {{ range .Site.RegularPages }}
    <url>
        <loc>{{ .Permalink }}</loc>
        {{ with .Lastmod }}
        <lastmod>{{ .Format "2006-01-02" }}</lastmod>
        {{ end }}
        <changefreq>weekly</changefreq>
        <priority>0.5</priority>
    </url>
    {{ end }}
</urlset>
